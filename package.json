    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^6.13.0"
  }
}
